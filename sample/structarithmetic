#include<stdio.h>
struct student
{
int id;
char no;
float marks;
};
int main()
{
struct student s1;
 struct student s2;
 struct student s3;

s1.id=23;
s1.no='a';
s1.marks=342.542f;
s2.id=24;
s2.no='b';
s2.marks=323.54f;
s3.id=s1.id*s2.id;
s3.no=s1.no*s2.no;
s3.marks=s1.marks*s2.marks;

printf("%d\n %d\n %f\n",s3.id,s3.no,s3.marks);

}